---
import config from "@/config";
import { Image } from "astro:assets";
import logo from "@/assets/logo.png";

const { title } = config.site;

interface Props {
  className?: string;
}

const { className } = Astro.props;
---

<Image
  height={80 / 2 * 1.2}
  src={logo}
  densities={[1, 2]}
  alt={title}
  loading="eager"
  class:list={["dark:invert", className]}
/>
